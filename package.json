    "@unocss/reset": "66.3.3",
    "astro": "^5.13.7",
    "astro-expressive-code": "^0.41.3",
    "astro-loader-bluesky-posts": "^1.2.2",
    "astro-loader-github-prs": "^1.2.1",
    "astro-loader-github-releases": "^2.0.2",
    "astro-robots-txt": "^1.0.0",
    "chalk": "^5.6.2",
    "del-cli": "^6.0.0",
    "eslint": "^9.35.0",
    "eslint-plugin-astro": "^1.3.1",
    "eslint-plugin-format": "^1.0.2",
