{
  "name": "astro-antfustyle-theme",
  "type": "module",
  "version": "3.0.0-beta.1",
  "private": true,
  "packageManager": "pnpm@10.16.0+sha512.8066e7b034217b700a9a4dbb3a005061d641ba130a89915213a10b3ca4919c19c037bec8066afdc559b89635fdb806b16ea673f2468fbb28aabfa13c53e3f769",
  "license": "MIT",
  "scripts": {
    "astro": "astro",
    "sync": "astro sync",
    "dev": "astro dev",
    "check": "astro check",
    "build": "astro build",
    "preview": "astro preview",
    "format": "prettier . --check --ignore-unknown",
    "format:write": "prettier . --write --ignore-unknown --cache",
    "lint": "eslint . --report-unused-disable-directives --max-warnings 0 --cache",
    "lint:fix": "eslint . --fix --cache",
    "toolbar:on": "astro preferences enable devToolbar",
    "toolbar:off": "astro preferences disable devToolbar",
    "prepare": "simple-git-hooks",
    "postbuild": "pagefind --site dist && del-cli \"dist/pagefind/*ui*\""
  },
  "dependencies": {
    "@astrojs/ts-plugin": "^1.10.4",
    "@mdx-js/typescript-plugin": "^0.1.3",
    "@types/mdast": "^4.0.4",
