    "vfile": "^6.0.3"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^5.3.0",
    "@ascorbic/feed-loader": "^1.0.4",
    "@astrojs/check": "^0.9.4",
    "@astrojs/markdown-remark": "^6.3.6",
    "@astrojs/mdx": "^4.3.5",
    "@astrojs/rss": "^4.0.12",
    "@astrojs/sitemap": "^3.5.1",
    "@atproto/api": "^0.15.27",
