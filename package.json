    "rehype-callouts": "^2.1.2",
    "rehype-external-links": "^3.0.0",
    "rehype-katex": "^7.0.1",
    "remark": "^15.0.1",
    "remark-directive": "^4.0.0",
    "remark-directive-sugar": "^1.1.2",
    "remark-imgattr": "^1.0.5",
    "remark-math": "^6.0.0",
    "satori": "^0.15.2",
    "satori-html": "^0.3.2",
    "sharp": "^0.34.3",
    "simple-git-hooks": "^2.13.1",
    "typescript": "^5.9.2",
